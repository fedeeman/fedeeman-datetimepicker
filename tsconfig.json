{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "fedeeman-datetimepicker/core": [
        "dist/core"
      ],
      "fedeeman-datetimepicker/core/*": [
        "dist/core/*"
      ],
      "fedeeman-datetimepicker/moment": [
        "dist/moment"
      ],
      "fedeeman-datetimepicker/moment/*": [
        "dist/moment/*"
      ]
    }
  }
}
